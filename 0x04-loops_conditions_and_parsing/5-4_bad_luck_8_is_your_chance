#!/bin/bash

# Initialize the loop counter
count=1

# Start the loop
while [ $count -le 10 ]; do
    # Check the current iteration number
    if [ $count -eq 4 ]; then
        echo "bad luck"
    elif [ $count -eq 8 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi
    
    # Increment the counter
    count=$((count+1))
done
